<!DOCTYPE html>
<html>
<head>

  <title> 跑过——记者站</title>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link rel="shortcut icon" href="/images/logo_orange.ico">

</head>
<body>
  <div class="container-fluid">
    <div class="row">
    <%= render "common/navbar" %>
    <%= render "common/flashes" %>
      <div class="col-md-2">
        <ul class="nav nav-pills nav-stacked" style="max-width: 300px;">
          <li> <%= link_to("产品模版", admin_products_path) %> </li>
          <li> <%= link_to("订单列表", admin_products_path) %> </li>
          <li> <%= link_to("跑记仓库", admin_products_path) %> </li>
        </ul>
      </div>
      <div class="col-md-10">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
